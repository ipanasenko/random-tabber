{
  "manifest_version": 3,
  "name": "Random Tabber",
  "version": "0.1.0",
  "description": "Jump to a random Chrome/Edge tab",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Random Tabber"
  },
  "permissions": ["storage"]
}
